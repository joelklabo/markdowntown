{
  "name": "markdowntown",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "E2E_BASE_URL=https://markdown.town vitest run __tests__/e2e/SectionFlow.test.ts",
    "test:coverage": "vitest run --coverage",
    "format:styles": "prettier src/app/globals.css --write",
    "docs:openapi": "node scripts/validate-openapi.js",
    "analyze": "ANALYZE=true pnpm exec next build --webpack"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/core": "^8.55.0",
    "@sentry/nextjs": "^8.55.0",
    "class-variance-authority": "^0.7.1",
    "next": "16.0.5",
    "next-auth": "^4.24.13",
    "posthog-js": "^1.200.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.4",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "jsdom": "^25.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.4.41",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.18.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "unified": "^11.0.5",
    "vitest": "^2.1.4"
  }
}
